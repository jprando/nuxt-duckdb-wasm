{
  "name": "duckdb-nuxt-wasm",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "dprint fmt",
    "duckdb:ui": "duckdb -cmd 'LOAD ui; CALL start_ui_server();'",
    "bun:compile": "bun build .output/server/index.mjs --compile --outfile nuxtbin",
    "prebun:compile": "NITRO_PRESET=bun NODE_OPTIONS=\"--max-old-space-size=8192\" bun run build"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.33.1-dev19.0",
    "@iconify-json/lucide": "^1.2.90",
    "@iconify-json/simple-icons": "^1.2.70",
    "@nuxt/hints": "1.0.0-alpha.6",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "3.23.0",
    "@nuxt/ui": "^4.4.0",
    "@unhead/vue": "^2.1.4",
    "css-tree": "^3.1.0",
    "csso": "^5.0.5",
    "mdn-data": "^2.27.1",
    "nuxt": "^4.3.1",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.15.1",
    "@playwright/test": "^1.58.2",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "dprint": "^0.51.1",
    "eslint": "^9.39.2",
    "playwright": "^1.58.2",
    "playwright-core": "^1.58.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^24.13.0",
    "npm": "^11.9.0"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc"
}
